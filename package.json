{
  "name": "stratocaster",
  "version": "0.0.0",
  "description": "Node.js and GraphQL API backend for a journaling application",
  "main": "index.js",
  "scripts": {
    "test:watch": "jest --watch",
    "test:ci": "jest",
    "lint": "flow && prettier-eslint --config .prettierrc.js --eslint-config-path .eslintrc.js --write  \"src/**.js\" && flow stop",
    "develop": "NODE_ENV=development nodemon src/index.js --exec babel-node src/index.js --presets @babel/preset-env,@babel/preset-flow",
    "build": "NODE_ENV=production babel src/index.js -o index.js",
    "commit": "npm run lint && npx git-cz"
  },
  "keywords": [
    "node",
    "api",
    "graphql",
    "journal",
    "markdown"
  ],
  "author": "Sean W. Lawrence",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "eslint": "^5.5.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-jest": "^21.22.0",
    "flow-bin": "^0.80.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1"
  },
  "dependencies": {
    "apollo-server": "^2.0.7",
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "pg-promise": "^8.4.6"
  }
}
